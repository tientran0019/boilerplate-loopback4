{
  "name": "simplize-trip-api",
  "version": "0.0.1",
  "description": "simplize trip api",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "posttest": "yarn run lint",
    "docker:build": "docker build -t simplize-trip-api .",
    "docker:run": "docker run -p 3000:3000 -d simplize-trip-api",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r tsconfig-paths/register -r source-map-support/register .",
    "predev": "yarn run clean",
    "dev": "tsc-watch --onSuccess \"node -r tsconfig-paths/register -r source-map-support/register --inspect .\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Tien Tran <tientran0019@gmail.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^6.1.3",
    "@loopback/core": "^5.1.3",
    "@loopback/repository": "^6.1.3",
    "@loopback/rest": "^13.1.3",
    "@loopback/rest-explorer": "^6.1.3",
    "@loopback/service-proxy": "^6.1.3",
    "app-root-path": "^3.1.0",
    "dotenv-extended": "^2.9.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "tslib": "^2.6.2",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.3",
    "@loopback/eslint-config": "^14.0.4",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "eslint": "^8.50.0",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.2.2"
  }
}
